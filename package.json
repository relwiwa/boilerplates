{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Relwiwa's boilerplate for React projects",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js --env production",
    "clean": "rimraf build",
    "deploy:gh-pages": "npm run build && ren build buildx && git checkout gh-pages && rimraf build && ren buildx build && git add . && git commit --message=\"Commit to gh-pages branch\" && git subtree push --prefix build github gh-pages && git checkout master",
    "deploy:surge": "surge -p build",
    "lint:js": "eslint src/  webpack.*.js --cache",
    "serve": "webpack-dev-server --env development --config webpack.config.js",
    "test": "jest"
  },
  "author": "Walter",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-react": "~6.24.1",
    "css-loader": "~0.28.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "~3.0.1",
    "file-loader": "~1.1.5",
    "html-webpack-plugin": "~2.30.1",
    "image-webpack-loader": "~3.4.2",
    "node-sass": "~4.5.3",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "~2.6.1",
    "sass-loader": "~6.0.5",
    "url-loader": "~0.6.2",
    "webpack": "~3.7.1",
    "webpack-dev-server": "~2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0"
  },
  "jest": {
    "rootDir": "tests",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/setup-tests.js"
    ]
  }
}
